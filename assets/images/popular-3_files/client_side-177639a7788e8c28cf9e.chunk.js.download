  @each $prop in $properties {\n    $str: #{$prop} #{$duration} #{$easing};\n    $list: join($list, #{$str}, comma);\n  }\n  transition: $list;\n}\n\nbody {\n  color: #000;\n  background: #f6f8fb;\n  font: 1.3rem/1.54 sans-serif;\n  min-width: 320px;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  @include media(">=tablet") {\n    font-size: 1.5rem;\n  }\n\n  @include media(">=widescreen") {\n    font-size: 2rem;\n  }\n  iframe[name="google_conversion_frame"] {\n    display: none;\n  }\n}\n\na {\n  @include animate(color background opacity box-shadow transform border);\n  text-decoration: none;\n  color: #ff5300;\n\n  &:hover {\n    color: $brand-primary-color;\n    text-decoration: none;\n  }\n}\n\nimg {\n  vertical-align: top;\n  max-width: 100%;\n}\n\nh1,\n.h1 {\n  @include media("<tablet") {\n    font-size: 2.4rem;\n  }\n\n  @include media("<phone") {\n    font-size: 1.8rem;\n  }\n}\n\nh2,\n.h2 {\n  @include media("<tablet") {\n    font-size: 2.2rem;\n  }\n\n  @include media("<phone") {\n    font-size: 1.6rem;\n  }\n}\n\nh3,\n.h3 {\n  @include media("<tablet") {\n    font-size: 2rem;\n  }\n\n  @include media("<phone") {\n    font-size: 1.5rem;\n  }\n}\n\nh4,\n.h4 {\n  @include media("<tablet") {\n    font-size: 1.8rem;\n  }\n\n  @include media("<phone") {\n    font-size: 1.3rem;\n  }\n}\n\nh5,\n.h5 {\n  @include media("<tablet") {\n    font-size: 1.6rem;\n  }\n\n  @include media("<phone") {\n    font-size: 1rem;\n  }\n}\n\nh6,\n.h6 {\n  @include media("<tablet") {\n    font-size: 1.5rem;\n  }\n\n  // @include media("<phone") {\n  //   font-size: 1rem;\n  // }\n}\n\nh3 {\n  color: #505050;\n}\n\nh2,\nh6 {\n  color: #3b3b3b;\n}\n\n// h1,\n// h2,\n// h3,\n// h4,\n// h5,\n// h6 {\n//   text-transform: none;\n// }\n\n%btn-reset {\n  @include animate(\n    color background opacity box-shadow transform background-position\n  );\n  display: inline-block;\n  vertical-align: middle;\n  border-radius: 2px;\n  appearance: none;\n  border: 0